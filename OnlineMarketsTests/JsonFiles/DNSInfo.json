{
  "Url": "https://www.dns-shop.ru/",
  "Pages": [
    {
      "Name": "ProductList",
      "XpathElements": { "findFieldOfSearch": "//div[contains(@class, 'menu')]//input[@name='q']" }
    },
    {
      "Name": "ProductDetails",
      "XpathElements": {
        "productId": "//a[contains(@class,'catalog-product__name')]",
        "nameOfProduct": "//h1[@class = 'product-card-top__title']",
        "buyButton": "//button[contains(@class,'button-ui buy-btn')]",
        "basketButton": "//span[@class = 'cart-link__price']",
        "nameOfProductInBasket": "//a[@class = 'cart-items__product-name-link']"
      }
    }
  ]
}
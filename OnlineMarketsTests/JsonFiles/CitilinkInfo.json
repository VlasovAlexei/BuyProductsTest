{
  "Url": "https://www.citilink.ru/",
  "Pages": [
    {
      "Name": "ProductList",
      "XpathElements": { "findFieldOfSearch": "//div[contains(@class, 'MainHeader__search')]//input[@name='text']" }
    },
    {
      "Name": "ProductDetails",
      "XpathElements": {
        "productId": "//div[@data-index='1']//a[contains(@class,' ProductCardVertical__link')]",
        "nameOfProduct": "//h1[@class = 'Heading Heading_level_1 ProductHeader__title']",
        "buyButton": "//div[contains(@class,'buy-button')]//button[contains(@class,'Button_with-icon')]",
        "basketButton": "//div[@class='UpsaleBasket__header-link']//button[contains(@class,'UpsaleBasket__order')]",
        "nameOfProductInBasket": "//a[@class='ProductCardForBasket__name  Link js--Link Link_type_default']"
      }
    }
  ]
}